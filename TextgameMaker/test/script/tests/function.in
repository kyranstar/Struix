// functions
def twice(n)
  temp is (n * 2); 
  return temp; 
end

def squared(n) 
  return n*n; 
end

def squaredAndTwice(n)
  return twice(squared(n));
end

assert(squared(666) equals 666^2);
assert(twice(squared(5)) equals 50);
//assert(squaredAndTwice(10) equals 200);
